# Backend
BACKEND_PORT=3000
NODE_ENV=development

# Database (host port 55432 is mapped to container 5432 in infra/docker-compose.yml)
DATABASE_URL=postgresql://postgres:postgres@localhost:55432/cwllm_db
DATABASE_URL_TEST=postgresql://postgres:postgres@localhost:55432/cwllm_db_test

# JWT (認証用、後で設定)
JWT_SECRET=your-secret-key-change-in-production

# Seed admin (後で設定)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123
ADMIN_ROLE=admin

# Chatwork API (後で設定)
CHATWORK_API_TOKEN=
CHATWORK_API_BASE_URL=https://api.chatwork.com/v2

# AI Provider (後で設定)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
# or
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_KEY=
